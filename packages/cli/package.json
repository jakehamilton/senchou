{
    "name": "@senchou/cli",
    "version": "0.0.0",
    "description": "A tool for generating Kubernetes manifests.",
    "main": "src/index.js",
    "scripts": {
        "clean": "rimraf ./dist",
        "build": "vite build",
        "build:types": "tsc ./src/index.ts --declaration --emitDeclarationOnly --outDir ./dist"
    },
    "keywords": [
        "kubernetes",
        "k8s",
        "cli",
        "generate",
        "leverage"
    ],
    "author": "Jake Hamilton <jake.hamilton@hey.com>",
    "license": "Apache-2.0",
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "bin/**/*",
        "dist/**/*",
        "CHANGELOG.md"
    ],
    "exports": {
        "import": "./dist/cli.es.js",
        "require": "./dist/cli.cjs.js",
        "node": "./dist/cli.cjs.js"
    },
    "dependencies": {
        "vite": "^2.8.0-beta.3",
        "typescript": "^4.6.0-dev.20220120",
        "@senchou/core": "^1.0.0",
        "@types/node": "^17.0.10",
        "rimraf": "^3.0.2"
    }
}
