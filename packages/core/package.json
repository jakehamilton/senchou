{
	"name": "@senchou/core",
	"version": "0.0.1",
	"description": "A library for generating Kubernetes manifests.",
	"main": "src/index.js",
	"scripts": {
		"clean": "rimraf ./dist",
		"build": "vite build",
		"build:types": "tsc ./src/index.ts --declaration --emitDeclarationOnly --outDir ./dist"
	},
	"keywords": [
		"kubernetes",
		"k8s",
		"cli",
		"generate",
		"leverage"
	],
	"author": "Jake Hamilton <jake.hamilton@hey.com>",
	"license": "Apache-2.0",
	"publishConfig": {
		"access": "public"
	},
	"files": [
		"bin/**/*",
		"dist/**/*",
		"CHANGELOG.md"
	],
	"exports": {
		"import": "./dist/core.es.js",
		"require": "./dist/core.cjs.js",
		"node": "./dist/core.cjs.js"
	},
	"dependencies": {
		"node-fetch": "^2.6.1",
		"js-yaml": "^4.1.0",
		"@littlethings/log": "^2.5.0",
		"json-schema": "^0.4.0",
		"@types/json-schema": "^7.0.9",
		"json2jsii": "^0.2.113",
		"codemaker": "^1.52.1",
		"@littlethings/coder": "^1.0.1",
		"@littlethings/schemer": "^1.0.1"
	},
	"devDependencies": {
		"@types/node": "^17.0.10",
		"@types/js-yaml": "^4.0.5",
		"@types/node-fetch": "^2.5.12",
		"vite": "^2.8.0-beta.3",
		"rimraf": "^3.0.2",
		"typescript": "^4.6.0-dev.20220120",
		"rollup-plugin-node-externals": "^3.1.2"
	}
}
